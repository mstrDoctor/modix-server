{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "providers": ["python"],
      "phases": {
        "install": {
          "cmds": [
            "pip install -r requirements.txt"
          ]
        },
        "start": {
          "cmds": [
            "python server.py"
          ]
        }
      }
    }
  }
}
